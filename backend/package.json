{
    "name": "snowit-rescue-backend",
    "version": "1.0.0",
    "description": "Backend for Snowit Rescue Checker",
    "main": "src/index.ts",
    "scripts": {
        "start": "ts-node src/index.ts",
        "dev": "nodemon src/index.ts",
        "build": "tsc",
        "dev:server": "npm run dev",
        "tunnel": "node scripts/update-tunnel.js",
        "share": "npx -y concurrently \"npm run dev\" \"npm run tunnel\""
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "date-fns": "^3.0.6"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.6",
        "nodemon": "^3.0.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}